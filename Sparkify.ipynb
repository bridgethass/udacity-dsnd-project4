    "# check the f1 score to see how we did:\n",
    "lr_f1_score = evaluator_test.evaluate(lr_pred, {evaluator_test.metricName: \"f1\"})\n",
    "print(\"Logistic Regression F1-Score: {:.2f}\".format(lr_f1_score))"
   ]
  },
